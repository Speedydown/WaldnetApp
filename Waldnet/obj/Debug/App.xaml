<Application
    x:Class="Waldnet.App"
    RequestedTheme="Light"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Waldnet">

    <Application.Resources>
        <SolidColorBrush x:Key="PivotHeaderForegroundSelectedBrush" Color="#FF07519A" />
        <SolidColorBrush x:Key="PivotHeaderForegroundUnselectedBrush" Color="#FFDCE7F3" />
        <x:String x:Key="ProgressBarIndeterminateForegroundThemeBrush">#FF07519A</x:String>
    </Application.Resources>


</Application>

